{
  "name": "neovimari",
  "version": "0.1.0",
  "private": true,
  "description": "Vim-style keyboard navigation for Safari",
  "license": "MIT",
  "author": "Neovimari contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/barryhall/neovimari.git"
  },
  "type": "module",
  "scripts": {
    "build": "npm run build:content && npm run build:background && npm run build:popup",
    "build:content": "vite build --config vite.content.config.ts",
    "build:background": "vite build --config vite.background.config.ts",
    "build:popup": "vite build --config vite.popup.config.ts",
    "dev": "concurrently \"vite build --config vite.content.config.ts --watch\" \"vite build --config vite.background.config.ts --watch\" \"vite build --config vite.popup.config.ts --watch\"",
    "check": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^9.0.0",
    "jsdom": "^25.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0",
    "vite-plugin-static-copy": "^2.0.0",
    "vitest": "^2.0.0"
  }
}
